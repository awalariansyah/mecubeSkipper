{
  "manifest_version": 2,

  "name": "Mecube Skipper",
  "version": "1.0.0",
  "description": "Youtube Ads Bypasser, mute unskippable ads, bypass banner ads and classic 5 seconds ads as well.",

  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["*://www.youtube.com/*","*://youtube.com/*","*://www.youtube.co.id/*","*://youtube.co.id/*"]
  }],
  
   "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}
